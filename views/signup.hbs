
<div class="container row">
            <div class="jumbotron col-sm-4 pull-center">
                <form class = "newUserForm" action="/signup" method="post">
                    <div class="input-field col s6">
                        <label>First Name:</label>
                        <input type="text" id="firstName" name="firstName" value= "test"/>
                    </div>
                    <div class="input-field col s6">
                        <label>Last Name:</label>
                        <input type="text" id="lastName" name="lastName" value= "test"/>
                    </div>
                    <div class="input-field col s6">
                        <label>Username:</label>
                        <input type="text"id="userName"  name="userName" value= "test"/>
                    </div>
